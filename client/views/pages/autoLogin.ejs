<script>
    run();

    function run () {
        let token = `<%= data.response.token %>`
        let displayName = `<%= data.response.displayName %>`
        let admin = `<%= data.response.admin %>`
        let success = `<%= data.response.success %>`

        admin = admin === "true";

        if (success) {
            window.localStorage.setItem("token", token);
            window.localStorage.setItem("admin", admin);
            displayName ? window.localStorage.setItem("displayName", displayName) : null;
        }
        window.location.href = window.location.origin + "/"
    }
</script>
